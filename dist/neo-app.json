{
	"authenticationMethod": "saml",
	"welcomeFile": "/index.html",
	"routes": [{
		"path": "/ESPM",
		"target": {
			"type": "destination",
			"name": "ESPMSSO"
		},
		"description": "ESPM"
	}, {
		"path": "/resources",
		"target": {
			"type": "service",
			"name": "sapui5",
			"entryPath": "/resources",
			"version": "1.38.11"
		},
		"description": "SAPUI5 Resources"
	}, {
		"path": "/test-resources",
		"target": {
			"type": "service",
			"name": "sapui5",
			"entryPath": "/test-resources",
			"version": "1.38.11"
		},
		"description": "SAPUI5 Test Resources"
	}],
	"sendWelcomeFileRedirect": true,
	"cacheControl": [{
		"directive": "private",
		"maxAge": 0,
		"path": "**.html"
	}, {
		"directive": "private",
		"maxAge": 0,
		"path": "sap-ui-cachebuster-info.json"
	}, {
		"directive": "public",
		"maxAge": 31536000
	}]
}